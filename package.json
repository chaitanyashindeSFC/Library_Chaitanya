{
  "name": "stfox-tailwind-react-ui",
  "version": "0.5.9",
  "description": "React + Tailwind component library (Flowbite-like)",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:js": "tsup src/index.ts --format cjs,esm --dts --sourcemap --clean --external react,react-dom",
    "build:css": "tailwindcss -c tailwind.config.cjs -i ./src/styles/tailwind.css -o ./dist/styles.css --minify",
    "build": "npm run clean && npm run build:js && npm run build:css",
    "dev": "concurrently \"tsup src/index.ts --watch --format cjs,esm --dts --sourcemap --external react,react-dom\" \"tailwindcss -c tailwind.config.cjs -i ./src/styles/tailwind.css -o ./dist/styles.css --watch\"",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "react": ">=18 <20",
    "react-dom": ">=18 <20"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^9.0.0",
    "postcss": "^8.4.0",
    "rimraf": "^6.0.0",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  },
  "dependencies": {
    "flowbite-react": "^0.12.10",
    "lucide-react": "^0.552.0"
  }
}
